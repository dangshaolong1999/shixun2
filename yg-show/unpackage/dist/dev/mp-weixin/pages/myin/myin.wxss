.share {
  padding: 15rpx;
  background-color: #fff;
}
.service {
  background-color: #fff;
}
.service view:nth-child(1) {
  display: flex;
  justify-content: space-between;
}
.service view {
  padding: 15rpx;
  border-bottom: 1px solid #ccc;
}
.address {
  background-color: #fff;
  padding: 15rpx;
}
.order {
  background-color: #fff;
}
.order .my {
  border-bottom: 1px solid #ccc;
  padding: 15rpx;
}
.order .flex {
  display: flex;
  justify-content: space-around;
  text-align: center;
  padding: 15rpx;
}
.order .flex view view:nth-child(1) {
  color: red;
}
.collection {
  background-color: #fff;
  margin-bottom: 50rpx;
  display: flex;
  justify-content: space-around;
  padding: 10rpx;
  text-align: center;
  margin-top: 50rpx;
  height: 90rpx;
  position: relative;
  z-index: 1;
}
.collection view view:nth-child(1) {
  color: red;
}
.a {
  width: 90%;
  margin: 30rpx auto;
}
.header1 {
  width: 100%;
  height: 500rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  overflow: hidden;
  margin-bottom: -75rpx;
}
.header1 image {
  width: 100%;
}
.header1 view {
  width: 130rpx;
  height: 200rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
}
.header1 view image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}
.box {
  background-color: rgba(199, 199, 199, 0.3);
  width: 100%;
  min-height: 120vh;
}
.header {
  margin-bottom: -75rpx;
  width: 100%;
  height: 500rpx;
  background-color: #eb4450;
  display: flex;
  justify-content: center;
  align-items: center;
}
.header navigator {
  color: #b3ea31;
  width: 115rpx;
  height: 90rpx;
  background-color: transparent;
  border: 1px solid #b3ea31;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 34rpx;
}